{
  "name": "react-starter-kit-rdb",
  "version": "0.1.0",
  "description": "React Starter Kit - Rethinkdb Isomorphic Edition",
  "main": "src/server.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack/server.js",
    "build-client": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config webpack/client.js",
    "clean": "rm -rf build",
    "lint": "./node_modules/eslint/bin/eslint.js -c .eslint src",
    "prebuild": "npm run clean",
    "postinstall": "npm run build",
    "start": "node build/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.js -w src/server.js -w src/server --exec babel-node --presets react,es2015,stage-0,react-hmre"
  },
  "author": "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse": "^4.2.0",
    "estraverse-fb": "^1.3.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.0",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "ejs": "^2.4.1",
    "ejs-compiled-loader": "^2.1.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "lodash": "^4.6.1",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-rethinkdb": "^0.5.4",
    "react-router": "^2.0.1",
    "react-transform-hmr": "^1.0.4",
    "rethinkdb-websocket-client": "^0.4.6",
    "rethinkdb-websocket-server": "^0.4.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14"
  }
}
